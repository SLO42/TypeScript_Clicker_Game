{
  "name": "typescript-backend",
  "version": "1.0.0",
  "description": "initial go at creating a typescript server for my clicker game.",
  "main": "src/server.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "docker-up": "docker compose --file ./docker/docker-compose.yml up",
    "docker-confirm-delete": "docker compose --file ./docker/docker-compose.yml down",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=dev nodemon --watch 'src/**/*.{js,ts}' --exec 'ts-node -T' src/server.ts",
    "db:migrate": "knex migrate:latest",
    "db:migrate:down": "knex migrate:down",
    "db:refresh": "knex migrate:down && knex migrate:latest",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage"
  },
  "author": "SLO42",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.6.1",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "google-auth-library": "^7.14.0",
    "helmet": "^5.0.2",
    "knex": "^1.0.3",
    "knex-paginate": "^3.0.0",
    "passport": "^0.5.2",
    "pg": "^8.7.3",
    "redis": "^4.0.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.19",
    "@types/pg": "^8.6.4",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.6"
  }
}
